{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [ 120 ]
        },
        "arrow-parens": false,
        "curly": false,
        "interface-name": false,
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": false,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-empty-interface": true,
        "no-empty": false,
        "object-literal-sort-keys": false,
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        }
    },
    "jsRules": {
        "max-line-length": {
            "options": [ 120 ]
        }
    }
}